    <tasktroubleshooting class="- topic/section task/tasktroubleshooting " xtrf="file:/Users/ekimber/workspace-ditaot/dita-ot-2.x/src/main/docsrc/dev_ref/using-plugin-URI-extension.dita" xtrc="tasktroubleshooting:1;59:26">
