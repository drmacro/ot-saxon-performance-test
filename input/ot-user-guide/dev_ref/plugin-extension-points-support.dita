      <codeblock xml:space="preserve" class="+ topic/pre pr-d/codeblock " xtrf="file:/Users/ekimber/workspace-ditaot/dita-ot-2.x/src/main/docsrc/dev_ref/plugin-extension-points-support.dita" xtrc="codeblock:2;40:18">&lt;plugin id="com.example.WithSupportInfo"&gt;
